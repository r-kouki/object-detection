# =============================================================================
# Environment Variables - Copy to .env and customize
# =============================================================================

# MinIO Configuration
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin

# PostgreSQL Configuration
POSTGRES_USER=mlflow
POSTGRES_PASSWORD=mlflow
POSTGRES_DB=mlflow

# MLflow Configuration
MLFLOW_TRACKING_URI=http://localhost:5000

# Model Serving
# Use one of:
#   - models:/detector/1  (Model Registry)
#   - runs:/<run_id>/artifacts/model  (Direct run artifact)
#   - /path/to/local/model.pt  (Local file)
MODEL_URI=models:/detector/1

# Grafana (optional)
GRAFANA_USER=admin
GRAFANA_PASSWORD=admin
